{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "schema": "https://schema.org/",
    "educertsterms": "https://som-bee.github.io/DID/educerts/terms#",

    "VerifiableCredential": "https://www.w3.org/2018/credentials#VerifiableCredential",
    "Ed25519Signature2020": "https://w3id.org/security#Ed25519Signature2020",
    "credentialSubject": {
      "@id": "https://www.w3.org/2018/credentials#credentialSubject",
      "@type": "@id"
    },
    "issuer": {
      "@id": "https://www.w3.org/2018/credentials#issuer",
      "@type": "@id"
    },
    "issuanceDate": {
      "@id": "https://www.w3.org/2018/credentials#issuanceDate",
      "@type": "xsd:dateTime"
    },
    "proof": {
      "@id": "https://w3id.org/security#proof",
      "@type": "@id"
    },
    "name": "schema:name",
    "description": "schema:description",
    "expirationDate": {
        "@id": "schema:expires",
        "@type": "xsd:dateTime"
    },
    "id": "@id",
    "type": "@type",

    "AdmitCard": {
      "@id": "#AdmitCardCredential"
    },
    "ExaminationCenter": {
      "@id": "#ExaminationCenterType"
    },
    "ExaminationSubject": {
      "@id": "#ExaminationSubjectType"
    },

    "candidateName": "schema:name",
    "fathersName": "educertsterms:fathersName",
    "mothersName": "educertsterms:mothersName",
    "dateOfBirth": {
      "@id": "schema:birthDate"
    },
    "medium": "schema:inLanguage",
    "stream": "educertsterms:academicStream",
    "gender": {
      "@id": "schema:gender"
    },
    "candidatePhoto": {
      "@id": "schema:image",
      "@type": "@id"
    },
    "candidatePhotoUrl": {
      "@id": "schema:url",
      "@type": "@id"
    },
    "rollNumber": "educertsterms:rollNumber",
    "enrollmentNumber": "educertsterms:enrollmentNumber",
    "centerNumber": "educertsterms:centerNumber",
    "schoolNumber": "educertsterms:schoolNumber",
    "formSerialNumber": "educertsterms:formSerialNumber",
    "candidateType": "educertsterms:candidateType",
    "examinationTime": "educertsterms:examinationTimeSlot",
    "examCenter": {
      "@id": "educertsterms:examinationCenter",
      "@type": "#ExaminationCenterType"
    },
    "examinationSchedule": {
      "@id": "educertsterms:examinationSchedule",
      "@container": "@list",
      "@type": "#ExaminationSubjectType"
    },
    "subjectCode": "educertsterms:subjectCode",
    "subjectName": "schema:name",
    "examDate": {
      "@id": "schema:startDate",
      "@type": "schema:Date"
    }
  }
}
