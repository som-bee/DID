{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "schema": "https://schema.org/",
    "educerts": "https://your-issuer-domain.com/terms#",
    "VerifiableCredential": "https://www.w3.org/2018/credentials#VerifiableCredential",
    "Ed25519Signature2020": "https://w3id.org/security#Ed25519Signature2020",
    "credentialSubject": {
      "@id": "https://www.w3.org/2018/credentials#credentialSubject",
      "@type": "@id"
    },
    "issuer": {
      "@id": "https://www.w3.org/2018/credentials#issuer",
      "@type": "@id"
    },
    "issuanceDate": {
      "@id": "https://www.w3.org/2018/credentials#issuanceDate",
      "@type": "xsd:dateTime"
    },
    "proof": {
      "@id": "https://w3id.org/security#proof",
      "@type": "@id"
    },
    "AdmitCard": {
      "@id": "educerts:AdmitCardCredential"
    },
    "ExaminationCenterDetails": "educerts:ExaminationCenterDetails",
    "ExamSubjectDetails": "educerts:ExamSubjectDetails",
    "candidateName": "schema:name",
    "fathersName": "educerts:fathersName",
    "mothersName": "educerts:mothersName",
    "dateOfBirth": {
      "@id": "schema:birthDate"
    },
    "medium": "schema:inLanguage",
    "stream": "educerts:academicStream",
    "gender": {
      "@id": "schema:gender"
    },
    "candidatePhoto": {
      "@id": "schema:image",
      "@type": "@id"
    },
    "candidatePhotoUrl": {
      "@id": "schema:url",
      "@type": "@id"
    },
    "rollNumber": "educerts:rollNumber",
    "enrollmentNumber": "educerts:enrollmentNumber",
    "centerNumber": "educerts:centerNumber",
    "schoolNumber": "educerts:schoolNumber",
    "formSerialNumber": "educerts:formSerialNumber",
    "candidateType": "educerts:candidateType",
    "examinationTime": "educerts:examinationTimeSlot",
    "examCenter": {
      "@id": "educerts:examinationCenter",
      "@type": "educerts:ExaminationCenterDetails"
    },
    "examinationSchedule": {
      "@id": "educerts:examinationSchedule",
      "@container": "@list",
      "@type": "educerts:ExamSubjectDetails"
    },
    "subjectCode": "educerts:subjectCode",
    "subjectName": "schema:name",
    "examDate": {
      "@id": "schema:startDate",
      "@type": "schema:Date"
    }
  }
}
